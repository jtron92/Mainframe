SELECT * FROM IBMUSER.MY_EMP;

SELECT FIRSTNME AS FIRST_NAME, EMPNO AS EMPLOYEE_NUMBER FROM IBMUSER.MY_EMP WHERE FIRSTNME IN ('JORGE','RAUL');

UPDATE IBMUSER.MY_EMP
SET JOB = 'SYS'
WHERE FIRSTNME = 'MARK';


INSERT INTO IBMUSER.MY_EMP (EMPNO,FIRSTNME,LASTNAME,MIDINIT)
VALUES ('0001','MARK','APPLE','S');


CREATE TABLE EMP_TEST (
    EMP_ID INT NOT NULL PRIMARY KEY,
    FIRST_NAME VARCHAR(50) NOT NULL,
    LAST_NAME VARCHAR(50),
    HIRE_DATE DATE NOT NULL,
    SALARY DECIMAL(10, 2)
);

TO ADD ANOTHER COLUMN**

ALTER TABLE EMP_TEST 
ADD TITLE VARCHAR(100);

